  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "e2e": "playwright test",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,html}",
    "lint": "eslint . && prettier --check src/**/*.{js,jsx,ts,tsx,html} && tsc --noEmit",
    "lint:fix": "eslint --fix . && prettier --write src/**/*.{js,jsx,ts,tsx,html}",
    "submit": "env-cmd -f .env.submit --silent -- tsx scripts/submit.ts",
    "submit:firefox": "env-cmd -f .env.submit --silent -- tsx scripts/submit.ts -b firefox",
    "test": "vitest",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "io-ts": "^2.2.21",
    "solid-js": "^1.8.11"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "@eslint/eslintrc": "^3.0.0",
    "@fast-check/vitest": "^0.0.9",
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.5",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "commander": "^11.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "^0.13.1",
    "globals": "^13.24.0",
    "playwright": "^1.41.0",
    "prettier": "^3.2.4",
    "publish-browser-extension": "^1.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.2.1",
    "wxt": "^0.14.3"
  },
  "packageManager": "pnpm@8.14.1",
  "volta": {
    "node": "20.11.0"
  }
}
