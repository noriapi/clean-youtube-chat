  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "e2e": "playwright test",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,html}",
    "lint": "eslint . && prettier --check src/**/*.{js,jsx,ts,tsx,html} && tsc --noEmit",
    "lint:fix": "eslint --fix . && prettier --write src/**/*.{js,jsx,ts,tsx,html}",
    "submit": "env-cmd -f .env.submit --silent -- tsx scripts/submit.ts",
    "submit:firefox": "env-cmd -f .env.submit --silent -- tsx scripts/submit.ts -b firefox",
    "test": "vitest",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "io-ts": "^2.2.21",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.16"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@eslint/eslintrc": "^3.0.2",
    "@fast-check/vitest": "^0.1.0",
    "@playwright/test": "^1.42.1",
    "@previewjs/cli": "^1.27.1",
    "@types/node": "^20.12.2",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "commander": "^12.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-solid": "^0.13.1",
    "globals": "^14.0.0",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "publish-browser-extension": "^1.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.7",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.4.0",
    "wxt": "^0.15.4"
  },
  "packageManager": "pnpm@8.15.5",
  "volta": {
    "node": "20.12.0"
  }
